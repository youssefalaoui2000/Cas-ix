{
  "name": "@mdline/mdline-parser",
  "version": "1.0.2",
  "description": "A parser for mdline.",
  "keywords": [
    "ast",
    "markdown",
    "mdline"
  ],
  "homepage": "https://github.com/azu/mdline/tree/master/packages/mdline-parser/",
  "bugs": {
    "url": "https://github.com/azu/mdline/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/mdline.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/mdline-parser.js",
  "types": "lib/mdline-parser.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "updateSnapshot": "cross-env UPDATE_SNAPSHOT=1 npm test",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@mdline/types": "^1.0.2",
    "mdast-util-to-string": "^1.0.5",
    "remark-html": "^9.0.0",
    "remark-parse": "^6.0.3",
    "remark-stringify": "^6.0.4",
    "select-section": "^0.4.6",
    "unified": "^7.1.0",
    "unist-util-select": "^1.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "ts-node-test-register": "^4.0.0",
    "typescript": "^3.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
